# .bashrc

# Source global definitions
if [ -f /etc/bashrc ]; then
	. /etc/bashrc
fi

# Uncomment the following line if you don't like systemctl's auto-paging feature:
# export SYSTEMD_PAGER=

# User specific aliases and functions

# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/home/wsr/anaconda3/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/home/wsr/anaconda3/etc/profile.d/conda.sh" ]; then
        . "/home/wsr/anaconda3/etc/profile.d/conda.sh"
    else
        export PATH="/home/wsr/anaconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<

export PATH="/home/wsr/anaconda3/bin:$PATH"

export PYTHONPATH="/home/wsr/:$PYTHONPATH"

export PYTHONPATH="/home/wsr/eda_gfea/:$PYTHONPATH"
export PYTHONPATH="/home/wsr/dftpart/:$PYTHONPATH"

# User specific aliases and functions
export PYTHONPATH=/home/wsr/wsrNN/andreweda1:$PYTHONPATH
#ase
export PYTHONPATH=/home/wsr/wsrNN/ase:$PYTHONPATH
#export PATH=/home/wsr/wsrNN/andreweda1/ase/ase:$PATH


